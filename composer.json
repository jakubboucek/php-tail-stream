{
    "name": "jakubboucek/php-tail-stream",
    "description": "PHP implementation of linux tail command, optimized for read huge size archives, effective memory usage.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub BoucÌŒek",
            "email": "pan@jakubboucek.cz"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "nette/tester": "2.5.x-dev",
        "phpstan/phpstan": "1.12.7"
    },
    "autoload": {
        "psr-4": {
            "JakubBoucek\\Tail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JakubBoucek\\Tail\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyze -c phpstan.neon --level 5",
        "test": "tester tests"
    }
}
